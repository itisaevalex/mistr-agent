{
  "servers": {
    "accuweather": {
      "type": "stdio",
      "name": "AccuWeatherServer",
      "command": "python",
      "args": ["-m", "mcp_weather"],
      "cwd": "mcp-servers/accuweather",
      "env": {
        "ACCUWEATHER_API_KEY": "YOUR_API_KEY_HERE",
        "PYTHONPATH": "./mcp-servers/accuweather"
      },
      "description": "AccuWeather hourly forecast service"
    }
  },
  "defaultServer": "accuweather"
}